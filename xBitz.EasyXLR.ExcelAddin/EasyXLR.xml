<?xml version="1.0" encoding="UTF-8"?>
<customUI onLoad="Ribbon_Load" xmlns="http://schemas.microsoft.com/office/2006/01/customui">
  <ribbon>
    <tabs>
      <tab idMso="TabAddIns" >
        <group id="ContentGroup" label="Project">
          <button id="textButton" label="New Project" onAction="btnAction"
               screentip="Button"
               supertip="Inserts text at the cursor location." tag="NEW"/>
          <button id="tableButton" label="Insert Table" onAction="btnAction"
               screentip="Table" tag="Refresh"
               supertip="Inserts a table at the cursor location."/>
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>

